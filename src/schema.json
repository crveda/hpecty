{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3,
      "description": "Please enter your name"
    },

    "Sex":{
      "type":"string",
      "enum":[
        "Male","Female"
      ]
    },

    "State":{
      "type":"string",
      "enum": [
        "KA",
        "MH",
        "UP",
        "DH",
        "TN",
        "Other"

      ]
    },

    "birthDate": {
      "type": "string",
      "format": "date"
    },

    "nationality": {
      "type": "string",
      "enum": [
        "IND",
        "ENG",
        "AUS",
        "US",
        "RU",
        "Other"
      ]
    },
    "age": {
      "type": "integer",
      "description": "Please enter your age.",
      "minimum":0,
      "maximum":100
    },
    "height": {
      "description": "Please enter your height in cm",
      "type": "number",
      "minimum":0,
      "maximum":300
    },
    "occupation":{
      "type":"string",
      "description": "Please enter your occupation."
    },
    "Postalcode":{
      "type":"string"
    }
  },
  "required": [
    "name",
    "birthDate",
    "occupation"
  ]
}